{
  "name": "layer_config",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "clean": "rm -rf build out",
    "export": "BUILD_OPTION=development next export",
    "export:prod": "BUILD_OPTION=production next export",
    "build": "yarn clean && next build && yarn export",
    "build:prod": "yarn clean && next build && yarn export:prod",
    "lint": "next lint",
    "format": "prettier --write --ignore-path .gitignore",
    "check-types": "bash -c tsc --noEmit",
    "prepare": "husky install",
    "test": "jest --env=jsdom --verbose"
  },
  "dependencies": {
    "@loaders.gl/core": "^3.0.9",
    "@loaders.gl/gltf": "^3.0.9",
    "amazon-cognito-identity-js": "^5.0.6",
    "aws-sdk": "^2.970.0",
    "axios": "^0.21.1",
    "deck.gl": "^8.5.5",
    "maplibre-gl": "^1.15.2",
    "next": "11.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-rangeslider": "^2.2.0"
  },
  "devDependencies": {
    "@danmarshall/deckgl-typings": "^4.9.4",
    "@testing-library/dom": "^8.14.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest": "^28.1.3",
    "@types/react": "17.0.17",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.3.1",
    "babel-jest": "^28.1.1",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "husky": "^7.0.0",
    "jest": "^28.1.1",
    "jest-css-modules": "^2.1.0",
    "jest-environment-jsdom": "^28.1.1",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "tailwindcss": "^2.2.7",
    "typescript": "4.3.5"
  },
  "lint-staged": {
    "*.@(ts|tsx|js|jsx|json)": [
      "yarn format",
      "yarn check-types"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "testEnvironment": "jest-environment-jsdom"
  }
}
